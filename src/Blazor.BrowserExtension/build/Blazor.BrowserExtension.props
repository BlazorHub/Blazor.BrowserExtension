<!-- build/Blazor.BrowserExtension.props -->
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <BrowserExtensionEnvironment Condition="'$(BrowserExtensionEnvironment)' == ''"></BrowserExtensionEnvironment>
    <IncludeBrowserExtensionAssets Condition="'$(IncludeBrowserExtensionAssets)' == ''">true</IncludeBrowserExtensionAssets>
    <LinkBrowserExtensionAssetsPath Condition="'$(LinkBrowserExtensionAssetsPath)' == ''">BrowserExtensionScripts</LinkBrowserExtensionAssetsPath>
    <BrowserExtensionAssetsPath Condition="'$(BrowserExtensionAssetsPath)' == ''">wwwroot</BrowserExtensionAssetsPath>
  </PropertyGroup>

</Project>